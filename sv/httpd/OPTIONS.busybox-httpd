name=httpd

SVC_CONFIGFILE=/etc/httpd.conf
SVC_CMD=busybox
SVC_OPTS="${name} -v -f -u ${SVC_USER}:${SVC_GROUP} -c ${SVC_CONFIGFILE} ${HTTP_OPTS}"

