#!/sbin/rs
#
# $Id: @(#) /etc/sv/sysctl                                            Exp $
# $L$: 2-clause/new/simplified BSD License                            Exp $
#

SVC_BEFORE="miscfs logger"
SVC_KEYWORD="prefix supervision systemd-nspawn vserver"
sysctl="$(command -v sysctl 2>${NULL})"

start_pre()
{
	[ -n "${sysctl}" -a -x "${sysctl}" ]
}
start()
{
	${sysctl} --quiet --system
}

#
# vim:fenc=utf-8:ft=sv:ci:pi:sts=0:sw=4:ts=4:
#
