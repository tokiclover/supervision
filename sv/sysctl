#!/sbin/rs
#
# $Header: /etc/sv/sysctl                                Exp $
# $License: 2-clause/new/simplified BSD                    Exp $
#

SVC_BEFORE="miscfs syslog"
sysctl="$(command -v sysctl 2>${NULL})"

start_pre()
{
	[ -n "${sysctl}" -a -x "${sysctl}" ]
}
start()
{
	${sysctl} --quiet --system
}

#
# vim:fenc=utf-8:ft=sv:ci:pi:sts=0:sw=4:ts=4:
#
