name=dhcpcd
SVC_AFTER='iptables ipt6tables'
SVC_PROVIDE='net'
#
# support instance for interface
#
case "${SVC_NAME}" in
	(*.[a-zA-Z]) DHCP_IFACES="${SVC_NAME#*.}";;
esac

