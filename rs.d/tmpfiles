#!/sbin/rs
#
# $Header: /etc/rs.d/zram                                Exp $
# $License: 2-clause/new/simplified BSD                  Exp $
#

description="Set up tmpfiles.d entries"

start()
{
	${SV_LIBDIR}/sh/tmpfiles --create --boot ${SVC_OPTS}
	end "${?}"
	return 0
}

